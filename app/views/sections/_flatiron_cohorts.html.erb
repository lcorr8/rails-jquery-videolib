<div class="container container-cohorts">
<center><h2>Flatiron Videos</h2></center>

  <div class="container cohorts cohort-fourteen text-center"> 
    <h3> 2014:</h3>
    <% @videos.fourteen.flatiron.each do |video| %>
      <p>
        <% if video.video_watched(@user, video) %>
          <%= fa_icon "check-square-o" %>
        <% end %>
        <%= link_to video.name, video_path(video) %>  
      </p>
    <% end %>  
  </div>

